{
	"name": "sticker-copy-bot",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"serve": "vite preview",
		"test": "vitest run",
		"migrate:local": "wrangler d1 migrations apply sticker-copy-bot --local",
		"migrate:remote": "wrangler d1 migrations apply sticker-copy-bot --remote",
		"deploy": "pnpm run migrate:remote && pnpm run build && wrangler deploy",
		"preview": "pnpm run build && wrangler preview",
		"cf-typegen": "wrangler types",
		"prepare": "husky && pnpm run migrate:local"
	},
	"dependencies": {
		"@cloudflare/vite-plugin": "^1.23.1",
		"@ffmpeg/ffmpeg": "^0.12.15",
		"@ffmpeg/util": "^0.12.2",
		"@tailwindcss/vite": "^4.1.18",
		"@tanstack/react-devtools": "^0.7.11",
		"@tanstack/react-router": "^1.159.4",
		"@tanstack/react-router-devtools": "^1.159.4",
		"@tanstack/react-router-ssr-query": "^1.159.4",
		"@tanstack/react-start": "^1.159.4",
		"@tanstack/router-plugin": "^1.159.4",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"grammy": "^1.39.3",
		"husky": "^9.1.7",
		"kysely": "^0.28.11",
		"kysely-d1": "^0.4.0",
		"lucide-react": "^0.544.0",
		"pretty-bytes": "^7.1.0",
		"radix-ui": "^1.4.3",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.18",
		"vite-tsconfig-paths": "^5.1.4",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@tanstack/devtools-vite": "^0.3.12",
		"@testing-library/dom": "^10.4.1",
		"@testing-library/react": "^16.3.2",
		"@types/node": "^22.19.10",
		"@types/react": "^19.2.13",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.3",
		"jsdom": "^27.4.0",
		"shadcn": "^3.8.4",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
